#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Users/samal/Projects/AshPicken/node_modules/.pnpm/registry.npmjs.org/sass/1.23.7/node_modules/sass/sass.js/node_modules:/mnt/c/Users/samal/Projects/AshPicken/node_modules/.pnpm/registry.npmjs.org/sass/1.23.7/node_modules/sass/node_modules:/mnt/c/Users/samal/Projects/AshPicken/node_modules/.pnpm/registry.npmjs.org/sass/1.23.7/node_modules:/mnt/c/Users/samal/Projects/AshPicken/node_modules/.pnpm/registry.npmjs.org/sass/node_modules:/mnt/c/Users/samal/Projects/AshPicken/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/Users/samal/Projects/AshPicken/node_modules/.pnpm/node_modules:/mnt/c/Users/samal/Projects/AshPicken/node_modules:/mnt/c/Users/samal/Projects/node_modules:/mnt/c/Users/samal/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/samal/Projects/AshPicken/node_modules/sass/sass.js/node_modules:/mnt/c/Users/samal/Projects/AshPicken/node_modules/sass/node_modules" "$basedir/node"  "$basedir/../sass/sass.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Users/samal/Projects/AshPicken/node_modules/.pnpm/registry.npmjs.org/sass/1.23.7/node_modules/sass/sass.js/node_modules:/mnt/c/Users/samal/Projects/AshPicken/node_modules/.pnpm/registry.npmjs.org/sass/1.23.7/node_modules/sass/node_modules:/mnt/c/Users/samal/Projects/AshPicken/node_modules/.pnpm/registry.npmjs.org/sass/1.23.7/node_modules:/mnt/c/Users/samal/Projects/AshPicken/node_modules/.pnpm/registry.npmjs.org/sass/node_modules:/mnt/c/Users/samal/Projects/AshPicken/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/Users/samal/Projects/AshPicken/node_modules/.pnpm/node_modules:/mnt/c/Users/samal/Projects/AshPicken/node_modules:/mnt/c/Users/samal/Projects/node_modules:/mnt/c/Users/samal/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/samal/Projects/AshPicken/node_modules/sass/sass.js/node_modules:/mnt/c/Users/samal/Projects/AshPicken/node_modules/sass/node_modules" node  "$basedir/../sass/sass.js" "$@"
  ret=$?
fi
exit $ret
